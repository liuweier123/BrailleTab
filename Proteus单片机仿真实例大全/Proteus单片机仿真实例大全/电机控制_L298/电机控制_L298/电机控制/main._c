//文件名 main.c 工程主文件

#include "config.h"


void main(void)
{

/******************************************************************************/
//初始工作
/******************************************************************************/
  init_devices();
  
  while(1)
  {
   for_ward(0);	 			//默认速度运转 正
   Delay1s(5); 				//延时5s
   motor_stop(); 			//停止
   Delay1s(5); 				//延时5s
   back_ward(0);			//默认速度运转 反
   Delay1s(5); 				//延时5s
   speed_add(20);			//加速
   Delay1s(5); 				//延时5s
   speed_subtract(20);		//减速
   Delay1s(5); 				//延时5s
  }
    
}